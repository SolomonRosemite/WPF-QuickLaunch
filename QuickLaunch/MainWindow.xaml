<Window x:Class="QuickLaunch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickLaunch"
        mc:Ignorable="d"
        MinHeight="200"
        MinWidth="200"
        Title="TMLaunch" Height="700" Width="450">
    <Grid>
        <Rectangle x:Name="backgroundColor" Stroke="#FFF7B6B6" Fill="#FFF7B6B6"/>
        <Button Content="Add" Click="AddApp" Margin="42,0,0,24" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="40"/>
        <Button Content="Edit" Click="EditApp" Margin="0,0,24,24" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="40"/>

        <ListView x:Name="listView" Margin="100"/>
    </Grid>
</Window>
